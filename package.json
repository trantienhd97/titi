{
  "name": "titi",
  "version": "1.0.0",
  "description": "Ứng dụng desktop đa nền tảng kết nối với SQL Server",
  "main": "main.js",
  "scripts": {
    "start": "electron . --no-sandbox",
    "dev": "electron . --dev --no-sandbox",
    "web": "node server.js",
    "web:dev": "NODE_ENV=development node server.js",
    "web:prod": "NODE_ENV=production node server.js",
    "build": "electron-builder --mac",
    "build-win": "electron-builder --win",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "electron",
    "desktop",
    "sql-server",
    "macOS",
    "windows"
  ],
  "author": "tientv@192",
  "license": "ISC",
  "devDependencies": {
    "electron": "^30.0.0",
    "electron-builder": "^24.9.1",
    "electron-reload": "^2.0.0-alpha.1"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "electron-fetch": "^1.9.1",
    "express": "^5.1.0",
    "express-fileupload": "^1.5.2",
    "path": "^0.12.7",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "tedious": "^18.6.1"
  },
  "build": {
    "appId": "com.tientv.titi",
    "productName": "titi",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "nsis"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trantienhd97/titi.git"
  },
  "bugs": {
    "url": "https://github.com/trantienhd97/titi/issues"
  },
  "homepage": "https://github.com/trantienhd97/titi#readme"
}
